/* Aggregating all of the styles from design. 
   Note that it was easier to do this here rather than importing styles as the image URLs 
   need to be changed to match up to confluence URLs */


/************* style.css*************************/
/*global*/
html, body {
    font-family:Arial, Helvetica, sans-serif;
    font-size:12px;    
    line-height:18px;    
    padding:0px;
    margin:0px;    
}
*     { margin: 0; padding: 0; }

/*Default Header*/
.header-wrapper {
    height:108px; 
    background-image:url(/plugins/servlet/builder/resource/HEADER/header-back.jpg); 
    background-repeat:repeat-x;
}
.header             {    width:1000px; margin:auto; padding-top:30px; }

/*Project Spaces Header*/
.projheader-wrapper {
    height:54px; 
    background-image:url(/plugins/servlet/builder/resource/HEADER/header_back_s.jpg); 
    background-repeat:repeat-x;
}
.projheader             {    width:1000px; margin:auto; padding-top:0px; }
/*end header*/

/*default*/
h1                {    font-size:24px; color:#000; margin-bottom:10px; font-weight:100; }
h2                         {    font-size:19px; margin-bottom:0px; padding-bottom:0px; font-weight:100; }
.green                     {    color:#336c24; }
h3                         {    font-size:16px; }
.floatright                 {    float:right; }
.floatleft                 {    float:left; }
.clearboth                 {    height:0px; clear:both; }
.clearleft                 {    clear:left; }
.clearright                 {    clear:right; }
.displayblocl             {    display:block;    }
/*end default*/
    
.separator                 {    border-top:1px solid #ccc; height:1px; background-color:#FFF; border-bottom:1px solid #fff; margin-top:10px; margin-bottom:10px; }
    
/*classes*/
.viewall, 
.viewall a, 
.viewall a:active, 
.viewall a:visited         {    color:#1a4e73; text-decoration:none; }
.viewall a:hover         {    text-decoration:underline; }

/*header*/
    
/*menu*/
.menu-back {    
    height:43px; 
    background-image:url(/plugins/servlet/builder/resource/HEADER/menu-back.jpg); 
    background-repeat:repeat-x;
} /* odd - confluence does whitespace trimming, which screws up this one image unless I add as resource */
.menu-wrapper            {    width:1000px; margin:auto; }
.menu-hor             {    margin:0px; padding:0px; }
.menu-hor li             {    float:left; list-style-type:none; padding:5px 25px; }
.menu-wrapper-left        {    float:left; }
.menu-wrapper-right {    
    float:right; 
    width:158px; 
    height:37px; 
    background-image:url(/plugins/servlet/builder/resource/HEADER/login-register.png); 
    position:relative; 
    bottom:12px; 
    text-align:center; 
    padding-top:19px;
}
.menu-wrapper-right, 
.menu-wrapper-right a, 
.menu-wrapper-right a:active, 
.menu-wrapper-right a:visited     {    color:#FFF; text-decoration:none; }
.menu-wrapper-right a:hover     {    text-decoration:underline; }

/*content*/
.content-wrapper {
    background-image:url(/plugins/servlet/builder/resource/HEADER/back-content.jpg); 
    background-repeat:repeat-x; 
    overflow:hidden; 
    width:100%;
}
.content                 {    width:1000px; margin:auto; }
.blue                {    color:#1a4e73; }
.displayblock            {    color:#919191; }
    
/* buttons and other related */
.buttom{
    background-image:url(/plugins/servlet/builder/resource/HEADER/btn.jpg);
    width:129px; 
    height:30px; 
    background-repeat:no-repeat; 
    text-align:center; 
    color:#ffffff; 
    display:block; 
    font-weight:bold; 
    font-size:12px; 
    line-height:29px; 
    text-decoration:none; 
    margin-top:35px;
}
.buttom a, .buttom a:visited, .buttom a:active {
    color:white;
}

.Bbuttom {
   border-top: 1px solid #96d1f8;
   background: #1388d6;
   background: -webkit-gradient(linear, left top, left bottom, from(#1a4e73), to(#1388d6));
   background: -webkit-linear-gradient(top, #1a4e73, #1388d6);
   background: -moz-linear-gradient(top, #1a4e73, #1388d6);
   background: -ms-linear-gradient(top, #1a4e73, #1388d6);
   background: -o-linear-gradient(top, #1a4e73, #1388d6);
   padding: 6.5px 13px 1px 1px;
   -webkit-border-radius: 8px;
   -moz-border-radius: 8px;
   border-radius: 8px;
   -webkit-box-shadow: rgba(0,0,0,1) 0 1px 0;
   -moz-box-shadow: rgba(0,0,0,1) 0 1px 0;
   box-shadow: rgba(0,0,0,1) 0 1px 0;
   text-shadow: rgba(0,0,0,.4) 0 1px 0;
   color: white;
   font-size: 14px;
   font-family: Georgia, serif;
   text-decoration: none;
   vertical-align: middle;
   }

.Bbuttom:hover {
   border-top-color: #28597a;
   background: #28597a;
   color: #ccc;
   }
.Bbuttom:active {
   border-top-color: #1b435e;
   background: #1b435e;
   }

/*footer*/
.footer-wrapper {    
    height:66px;     
    background-image:url(/plugins/servlet/builder/resource/HEADER/footer.jpg);     
    background-repeat:repeat-x;  
    margin-top:25px;
}
.footer             {    color:#FFFFFF; padding-top:25px; width:1000px; margin:auto; }
.footer-l            {    float:left; }
.footer-l a             {    color:#FFF; text-decoration:none; }
.footer-r             {    float:right; }
.footer-r a             {    color:#ffffff; text-decoration:none; }

/*content*/
.latest-blog-post         {    float:left; width:636px; padding-right:18px; border-right:1px solid #ccc; }
.author             {    float:left; color:#565656; }
.date                         {    float:right; color:#565656; }

.recent-activities         {    float:right; width:330px; text-align:left; }
.activity-item                {    padding-bottom:21.5px; border-bottom:1px solid #ccc; padding-top:20px; }

/************* jqueryslidemenu.css*************************/
.jqueryslidemenu{
    font: normal 12px Verdana;
    height:43px; 
    background-image:url(/plugins/servlet/builder/resource/HEADER/menu-back.jpg); 
    background-repeat:repeat-x;)
    width: 100%;
}

.jqueryslidemenu ul {
    margin: 0;
    padding: 0;
    list-style-type: none;
}

/*Top level list items*/
.jqueryslidemenu ul li{
    position: relative;
    display: inline;
    float: left;
}

/*Top level menu link items style*/
.jqueryslidemenu ul li a{
    display: block;
    background: transparent; /*background of tabs (default state)*/
    color: white;
    padding: 8px 10px;
    border-right: 1px solid #7c9eb6;
    border-left:1px solid #000;
    color: #2d2b2b;
    text-decoration: none;
}

.jqueryslidemenu ul li a.menubarfirst    {    border-left:0px; }
.jqueryslidemenu ul li a.menubarlast    {    border-right:0px; }

* html .jqueryslidemenu ul li a    { /*IE6 hack to get sub menu links to behave correctly*/
    display: inline-block;
}

.jqueryslidemenu ul li a:link, .jqueryslidemenu ul li a:visited {
    color: white;
}

.jqueryslidemenu ul li a:hover{
    background: url(/plugins/servlet/builder/resource/HEADER/menu-hover.jpg); /*tab link background during hover state*/
    color: white;
}
    
/*1st sub level menu*/
.jqueryslidemenu ul li ul{
    position: absolute;
    left: 0;
    display: block;
    visibility: hidden;
    z-index:10;
}

/*Sub level menu list items (undo style from Top level List Items)*/
.jqueryslidemenu ul li ul li{
    display: list-item;
    float: none;
    background-image:url(/plugins/servlet/builder/resource/HEADER/submenu-back.jpg)
}

/*Sub level menu list items (first itme, and item separator) */
.jqueryslidemenu ul li ul li.menuoptfirst {
    height:9px;
    background-image:url(/plugins/servlet/builder/resource/HEADER/submenu-top.jpg); 
    margin-top:-1px;  
}
.jqueryslidemenu ul li ul li.menuseparator {
    height:9px;
    background-image:url(/plugins/servlet/builder/resource/HEADER/submenu-sep.jpg); 
}

/*All subsequent sub menu levels vertical offset after 1st level sub menu */
.jqueryslidemenu ul li ul li ul{
    top:100px;
}

/* Sub level menu links style */
.jqueryslidemenu ul li ul li a {
    width: 160px; /*width of sub menus*/
    padding: 8px;
    margin: 0;
}

.jqueryslidemenuz ul li ul li a:hover{ /*sub menus hover style*/
    background-color:#ffffff;
    color: black;
}

.jqueryslidemenu ul li ul li a:hover{
    background-color:#FF0;
    color: white;
}

/*  CSS classes applied to down and right arrow images  */
.downarrowclass{
    position: absolute;
    top: 12px;
    right: 7px;
}
.rightarrowclass{
    position: absolute;
    top: 6px;
    right: 5px;
}


