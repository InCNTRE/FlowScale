div.main_page_container {
    background: url(../media/oess-frame-body.png) repeat-y bottom;
    width: 1024px;
    text-align: left;
}

div.main_page_header {
    background: url(../media/oess-frame-head.png) no-repeat bottom;
    height: 70px;    
    width: 1024px;
}

div.main_page_footer {
    background: url(../media/oess-frame-footer.png) no-repeat bottom;
    height: 40px;
    width: 1024px;
}

body {
    font-family: Arial,Helvetica,sans-serif !important;    
    background: url(../media/background-gradient.png) repeat-x top;
}

a.network_link{
    top: 5px;
}

p.title {
    font-family: Arial Black,Helvetica,sans-serif !important;
    margin: 0px;
}

p.subtitle {
    color: grey;
    font-size: 85%;
    margin: 0px;
}

p.soft_title {
    margin: 0px;
    font-weight: bold;
    color: grey;
}

div.openlayers{
    width: 558px;
    height: 300px;	
    border: 1px solid #cccccc;
}

div.top_nav {
    padding-top: 50px;
}

div.breadcrumbs {
    text-align: left;
    float: left;
    font-size: 88%;
    margin-left: 35px;
}

div#page_summary_container {
    float: left; 
    margin-top: 10px;
    margin-left: 20px;
    text-align: left;
}

div#next_button_container {
    float: right; 
    padding-right: 40px; 
    padding-top: 17px;
    font-size: 90%;
}

div#view_details_buttons_div {
    float: right;
    width: 130px;
    margin-right: 100px;
    margin-top: -150px;
}

div#view_details_buttons_div button {
    width: 120px;
}

div#view_details_buttons_div .button_holder {
    padding-bottom: 100px;
}

.yui-skin-sam .yui-button, .yui-skin-sam .yui-button .first-child {
    border-radius: 10px;
}



button#button_class {
	
	font-weight: bold;
}

div.main_page {
    width: 1024px;
    clear: both;
    min-height: 500px;
    margin-left: 26px;
}

div.admin {
    margin-left: 0px;
}

div#footer {
    width: 1024px;
    text-align: center;
}

div.map_legend table {
    font-size: 10px;
    font-style: italic;
}

div.map_legend table td img {
    height: 15px;
    width: 15px;
}

div.map_legend table td div {
    height: 12px;
    width: 12px;
    float: left;
    margin-right: 5px;
//    border-radius: 10px;
}

div.provision_time_selection {
    margin-left: 10px;
    padding: 8px;
    background-color: #F8F8C6;
}

span.breadcrumb_spacer {
    padding-left: 4px;
    padding-right: 4px;
}

span.disabled_breadcrumb {
    color: #474747;
    text-decoration: line-through;
}

span.selected_breadcrumb {
    font-weight: bold;
    color: white;
    font-size: 105%;
}

span.future_breadcrumb {
    color: #ECECEC;
}

table.map_table td {
    padding-right: 10px;
    padding-bottom: 4px;
}

table.nicely_spaced td{
    padding-top: 10px;
    padding-left: 10px;
}

table.circuit_summary_table {
    width: 800px;
}

table.circuit_summary_table td{
    padding-top: 4px;
    padding-left: 6px;
}


label {
	font-weight: bold;
	padding: 2px;
}

#button_class{
	font-weight: bold;
	
}

#sliderbg { 
    position: relative; 
    background:url(../media/bg-fader.gif) 5px 0 no-repeat; 
    height:28px; 
    width:228px;  
    float:left;
} 

#slider-value{
    float:right;
}

#sliderthumb { 
    position: absolute; 
    top: 6px; 
}

#circuit_endpoints_table {

}

#circuit_endpoints_table th{
    font-size: 11px;    
}

#circuit_endpoints_table.yui-dt-scrollable .yui-dt-bd {
    border-left: 0;
    border-right: 0;
    border-bottom: 0;
}

#circuit_endpoints_table .yui-dt-hd {
    width: 347 !important;
}

#circuit_endpoints_table.yui-dt tr td.yui-dt-first {
    border-left: 1px solid #CBCBCB;
}

#circuit_endpoints_table .yui-dt-message td {
    border-bottom: 1px solid #CBCBCB;
}

.yui-skin-sam .yui-dt-scrollable .yui-dt-data tr.yui-dt-last td {
    border-bottom: 1px solid #CBCBCB;
}

.yui-skin-sam tr.yui-dt-odd {
    background-color: #EFEFEF;
}

.yui-skin-sam tr.yui-dt-highlighted {
    background-color: #B2D2FF
}

.olPopup {
    border-radius: 10px;
    opacity: 0.8 !important;
    filter:alpha(opacity=80) !important;
}