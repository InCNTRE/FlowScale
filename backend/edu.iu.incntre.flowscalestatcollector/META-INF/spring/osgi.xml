<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:osgi="http://www.springframework.org/schema/osgi"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/osgi
    http://www.springframework.org/schema/osgi/spring-osgi-1.2.xsd">

  <osgi:reference id="flowscaleController" cardinality="1..1"
    interface="edu.iu.incntre.flowscale.FlowscaleController">
  </osgi:reference>
  

  
  <osgi:service id="StatCollector" ref="statCollector"
    interface="edu.iu.incntre.flowscalestatcollector.StatCollector">
  </osgi:service>


</beans>